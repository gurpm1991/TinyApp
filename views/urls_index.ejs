<!DOCTYPE html>
<html>
	<head>
  		<title>TinyApp</title>
	</head>
	<body>
		<%- include ./partials/_header %>
    <% for (var key in urls) { %>
		<form action="/urls/<%= key %>/delete" method="POST" style="margin:15px;">
      		<label ><%= key %>:</label>
      		<label ><%= urls[key].longURL %></label>
      		<label ><%= urls[key].id %></label>
      		<input type="submit" value="DELETE">
      		<a href="/urls/<%=key %>"><button type="button">EDIT</button></a>
    </form>
		<% } %>  
	</body>
</html>


